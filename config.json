{
  "=": {
    "alignment":  "left",
    "leftSpace":  true,
    "rightSpace": true,
    "prefixes":   ["+", "-", "&", "|", "<", ">", "!", "~", "%", "/", "*", "."],
    "scope":      "operator|assignment"
  },
  ":": {
    "alignment":  "right",
    "leftSpace":  false,
    "rightSpace": true,
    "scope":      "operator|assignment"
  },
  ",": {
    "leftSpace":  true,
    "rightSpace": false,
    "scope":      "delimiter",
    "multiple":   {
      "number": {
        "alignment": "left"
      },
      "string": {
        "alignment": "right"
      }
    }
  },
  "=>": {
    "alignment":  "left",
    "leftSpace":  true,
    "rightSpace": true,
    "scope":      "key-value"
  }
}
